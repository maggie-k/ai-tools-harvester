{
  "actorOutputSchemaVersion": 1,
  "title": "AI Tools Harvester Output",
  "description": "Results of AI tool recommendations",
  "properties": {
    "dataset": {
      "title": "Recommended AI Tools",
      "type": "dataset",
      "description": "Dataset containing AI tools recommended for the input query",
      "views": {
        "overview": {
          "title": "AI Tools Overview",
          "transformation": {
            "fields": [
              "name",
              "website",
              "category",
              "score"
            ]
          },
          "display": {
            "component": "table",
            "properties": {
              "name": {
                "label": "Tool Name",
                "format": "text"
              },
              "website": {
                "label": "Website",
                "format": "link"
              },
              "category": {
                "label": "Category",
                "format": "text"
              },
              "score": {
                "label": "Relevance Score",
                "format": "number"
              }
            }
          }
        }
      }
    }
  }
}
